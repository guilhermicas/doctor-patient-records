extends base
block head
  link(rel="stylesheet", href="/public/css/base.css")
block body
  script(src="./public/js/base.js", type="text/javascript", defer)
  h5 Pacientes Inseridos
  if err
    h3(class="msgErro") #{err}
  div(class="container-items")
    if paciente
      div(class="container-paciente")
        h2 #{paciente.nome}
        textarea(readonly) #{paciente.descricao}
        h2 #{paciente.created_at}
        
  input(type="button",value="logout",class="logoutBtn")
